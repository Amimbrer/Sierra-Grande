{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "pluginOptions": {},
      "type": "string",
      "minLength": 1,
      "maxLength": 100,
      "required": true
    },
    "slug": {
      "pluginOptions": {},
      "type": "uid",
      "targetField": "Nombre",
      "required": false
    },
    "Precio": {
      "pluginOptions": {},
      "type": "decimal",
      "min": 1,
      "required": true
    },
    "Activo": {
      "pluginOptions": {},
      "type": "boolean",
      "default": true,
      "private": true
    },
    "Stock": {
      "pluginOptions": {},
      "type": "boolean",
      "default": true
    },
    "StockLimitado": {
      "pluginOptions": {},
      "type": "integer",
      "min": 1
    },
    "tipo_productos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tipo-producto.tipo-producto",
      "inversedBy": "productos"
    },
    "Imagenes": {
      "allowedTypes": [
        "images",
        "videos"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    }
  }
}
